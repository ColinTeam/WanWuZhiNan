<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/fragment_home">
    <action
        android:id="@+id/action_toLogin"
        app:destination="@+id/fragment_login" />
    <action
        android:id="@+id/action_toHome"
        app:destination="@+id/fragment_home" />
    <action
        android:id="@+id/action_toWeb"
        app:destination="@+id/fragment_web" />
    <fragment
        android:id="@+id/fragment_login"
        android:name="com.wanwuzhinan.mingchang.ui.fragment.LoginFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_login" />
    <fragment
        android:id="@+id/fragment_home"
        android:name="com.wanwuzhinan.mingchang.ui.fragment.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/fragment_web"
        android:name="com.wanwuzhinan.mingchang.ui.fragment.WebFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_web">
        <argument
            android:name="url_type"
            android:defaultValue="-1"
            app:argType="integer" />
        <argument
            android:name="h5_url"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="web_title"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="web_content"
            app:argType="string"
            app:nullable="true" />
    </fragment>

</navigation>